import { IApp } from '@/types/nuxt'
import <%= name %>Entity from '@/entities/<%= name %>'
import RefreshUseCase from '@/usecases/RefreshUseCase'

export default class Delete<%= name %>UseCase extends RefreshUseCase implements BaseUseCase {
  app: IApp
  constructor(app: IApp) {
    super()
    this.app = app
  }

  async execute(entity: <%= name %>Entity) {
    await super.refresh(this.app)
    await this.app.<%= appName.toLowerCase() %>Gateway.<%= name %>.Delete<%= name %>(entity)
  }
}
